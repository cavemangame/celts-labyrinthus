<Page x:Class="Labyrinthus.Pages.PageShow3DLabyrinthus"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="PageShow3DLabyrinthus"
      Loaded="Page_Loaded">

    <Grid>
        <Viewport3D Name="LabyrinthusViewport" Width="500" Height="500">
            <Viewport3D.Camera>
                <PerspectiveCamera Position="-3,-3,5"
                                   LookDirection="1,1.2,-1"
                                   UpDirection="0,0,1"/>
            </Viewport3D.Camera>
        </Viewport3D>
    </Grid>
</Page>
